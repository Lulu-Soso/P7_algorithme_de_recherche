        <mxCell id="kESQg2O9y7uP-aah3dOf-3" value="" style="shape=image;verticalLabelPosition=bottom;labelBackgroundColor=default;verticalAlign=top;aspect=fixed;imageAspect=0;image=data:image/png,;" vertex="1" parent="kESQg2O9y7uP-aah3dOf-1">
