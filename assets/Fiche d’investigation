        <mxCell id="XINpk7pVPMcNPmLBe9os-2" value="&lt;font style=&quot;font-size: 18px;&quot;&gt;&lt;u&gt;&lt;b&gt;Fiche d&#39;investigation: Rechercher des recettes&lt;/b&gt;&lt;/u&gt;&lt;/font&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;strokeColor=none;fillColor=none;" vertex="1" parent="XINpk7pVPMcNPmLBe9os-1">
          <mxGeometry x="204" y="40" width="420" height="40" as="geometry" />
